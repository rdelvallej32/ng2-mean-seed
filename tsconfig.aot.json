{
    "compilerOptions": {
        "module": "es2015",
        "target": "es5",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "outDir": "./dist/client/app/js"
    },
    "exclude": [
        "node_modules",
        "dist",
        "client/app/polyfills.ts"
    ],
    "angularCompilerOptions": {
      "skipMetadataEmit": true
    }
}
